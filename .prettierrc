{
  "trailingComma": "es5",
  "tabWidth": 4,
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "jsxBracketSameLine": true,
  "bracketSpacing": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "sortImports": true,
  "sortImportsSeparately": true,
  "importOrder": [
    "^(react|react-native|react-redux)(/.*|$)",
    "^../../assets/oldImg/",
    "^../screens/",
    "^../components/",
    "^./",
    ".*"
  ],
  "importOrderSeparation": true,
  "importsSeparated": true,
  "separateGroups": true,
  "separateStyle": "-----"
}
